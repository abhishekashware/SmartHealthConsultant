            android:parentActivityName=".smarthealthconsultant.Main_Activity.MainActivity" />
        <activity
            android:name=".smarthealthconsultant.Doctor.DoctorList"
            android:parentActivityName=".smarthealthconsultant.PatientDirectory.PatientHome" />
        <activity
            android:name=".smarthealthconsultant.Walkthrough.WelcomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>